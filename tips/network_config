# 命令行配置网络信息
1./etc/network/interfaces
auto eth0
iface eth0 inet static
	address 192.168.2.123
	netmask 255.255.255.0
	# network 192.168.2.0
	broadcast 192.168.2.255
	gateway 192.168.2.1
	# dns-* optoinos are implemented by the resolvconf package, if installed
	# dns-nameservers 8.8.8.8 这个似乎没有用
2./etc/resolv.conf
nameserver 192.168.2.1
